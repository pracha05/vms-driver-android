<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <rotate android:fromDegrees="50" android:toDegrees="45" android:pivotX="150%" android:pivotY="10%">
            <shape android:shape="rectangle">
                <solid android:color="#ec3636"/>
            </shape>
        </rotate>
    </item>
    <item>
        <rotate android:fromDegrees="50" android:toDegrees="90" android:pivotX="75%" android:pivotY="100%">
            <shape android:shape="rectangle">
                <solid android:color="#ec3636"/>
            </shape>
        </rotate>
    </item>
</layer-list>
